<?xml version="1.0"?>
<!-- {{ managedText | raw }} -->

<ruleset name="{{ name }} coding standards">
   <description>PHPCS configuration file for {{ name }}.</description>
   <file>src</file>
   <arg value="np"/>
   {% for standard in php.phpcs.standards|default(['Drupal', 'DrupalPractice']) %}
   <rule ref="{{ standard }}"/>
   {% endif %}
</ruleset>
