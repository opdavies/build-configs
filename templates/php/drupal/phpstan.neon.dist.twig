# {{ managedText | raw }}

parameters:
  level: {{ php.phpstan.level }}
{% if not experimental.runStaticAnalysisOnTests %}
  excludePaths:
    - *Test.php
    - *TestBase.php
{% endif %}
  paths:
    {% for path in php.phpstan.paths -%}
    - {{ path }}
    {%- endfor %}

{% if php.phpstan.baseline %}
includes:
  - phpstan-baseline.neon
{% endif %}
